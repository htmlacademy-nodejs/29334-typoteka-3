  "devDependencies": {
    "eslint": "^7.11.0",
    "eslint-config-htmlacademy": "0.6.0",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "pino-pretty": "4.7.1",
    "supertest": "6.1.3"
  },
  "dependencies": {
    "cross-env": "7.0.3",
